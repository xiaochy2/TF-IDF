<?xml version='1.0' encoding='ISO-8859-1'?> 
<!-- index: contains all tokens -->
<!-- w: stand for single token -->
<!-- n: name of the token -->
<!-- ds: stand for documents that contain the token, one token can exists in lots of documents -->
<!-- d: stand for one single document that contains this token-->
<!-- dn: one single document name-->
<!-- tf-idf: tf-idf -->
<!-- p: position -->
<!-- wp: word position -->
<!-- lp: letter position-->
<index>
    <w>
        <n>0</n>
		<ds>
			<d>
				<dn>2/5</dn>
				<tf-idf></tf-idf>
				<p>
					<wp>196</wp>
					<lp>201</wp>
					
				</p>
				<p>
					<wp>2195</lp>
					<lp>2209</lp>
					
				</p>
			</d>


			<d>
				...
			</d>

		</ds>
	</w>


	<w>
		...
	</w>
</index>

